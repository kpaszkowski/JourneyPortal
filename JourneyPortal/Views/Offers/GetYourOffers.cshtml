@model List<JourneyPortal.ViewModels.Offers.CreateOfferDetailViewModel>
@{

}


<h3>Twoj oferty</h3>

<div>
    <ul>
        @foreach (var item in Model)
        {
            <li>
                <h3>@item.Name</h3><br />
                <span>@item.Description</span><br />
                @item.StartDate - @item.EndDate<br />
                Dodana @item.CreationDate<br />
                @item.NuberOfBooking<br />
                @item.CreationDate<br />
                @item.Cost zł<br />

            </li>
        }
    </ul>
</div>

