@model JourneyPortal.Models.TravelAgencyInfoViewModel

@{
    ViewBag.Title = "GetTravelAgencyProfile";
}

<h2>@Model.Name</h2>

<div>
    <ul>



        @foreach (var item in Model.OffersList)
        {
            <li>
                <h3>@item.Name</h3><br />
                <span>@item.Description</span><br />
                @item.StartDate - @item.EndDate<br />
                Dodana @item.CreationDate<br />
                @item.NuberOfBooking<br />
                @item.CreationDate<br />
                @item.Cost zł<br />
            </li>
        }
    </ul>
</div>
<p>
    @Html.ActionLink("Back to List", "Index","Offers")
</p>
